<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression Suite" thread-count="3" parallel="tests" verbose="4">

	<test name="Amazon Test firefox">
		<parameter name="url" value="https://www.amazon.com" />
		<parameter name="browser" value="firefox" />

		<classes>
			<class name="testcases.AmazonTest" />
		</classes>
	</test>
	<test name="Amazon Test chrome">
		<parameter name="url" value="https://www.amazon.com" />
		<parameter name="browser" value="chrome" />

		<classes>
			<class name="testcases.AmazonTest" />
		</classes>
	</test>
		<test name="Amazon Test edge">
		<parameter name="url" value="https://www.amazon.com" />
		<parameter name="browser" value="edge" />

		<classes>
			<class name="testcases.AmazonTest" />
		</classes>
	</test>

</suite>